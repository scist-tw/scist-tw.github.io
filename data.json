          ]
        }
      ],
      "middle": [
        {
          "name": "下學期 上學期",
          "topics": [
            {
              "name": "基礎資料結構與演算法",
              "subtopics": [
                "複雜度分析",
                "STL 資料結構",
                "遞迴",
                "排序",
                "搜尋",
                "單調性與差分"
              ]
            }
          ]
        }
      ],
      "advanced": [
        {
          "name": "下學期",
          "topics": [
            {
              "name": "進階資料結構與演算法",
              "subtopics": [
                "DP 動態規劃",
                "Greedy 貪心法",
                "基礎數論",
                "基礎圖論、BFS & DFS",
                "進階資料結構",
                "RMQ"
              ]
            }
          ]
        }
      ]
